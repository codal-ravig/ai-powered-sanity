{
  "name": "my-bakery",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "typegen": "sanity schema extract && sanity typegen generate --enforce-required-fields",
    "seed": "pnpm dlx tsx generate-seed.ts",
    "import:data": "pnpm dlx sanity dataset import generated-seed.ndjson production --replace",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/migrate": "^5.2.4",
    "@sanity/vision": "^5.10.0",
    "react": "^19.1",
    "react-dom": "^19.1",
    "sanity": "^5.10.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.3.0",
    "@sanity/eslint-config-studio": "^6.0.0",
    "@types/react": "^19.1",
    "eslint": "^9.28",
    "prettier": "^3.5",
    "typescript": "^5.8"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
